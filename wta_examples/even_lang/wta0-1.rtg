// The example graph from the "Best trees" paper

a -> pa # 2
a -> pb # 1
b -> pa # 1
b -> pb # 2

ball[pa, pa] -> qa
ball[qa, qa] -> qa

ball[pb, pb] -> qb
ball[qb, qb] -> qb

ball[pa, qa] -> pa
ball[qa, pa] -> pa

ball[pb, qb] -> pb
ball[qb, pb] -> pb

final qa, qb

// 2 leaves

a -> pa1 # 2
a -> pb1 # 1
b -> pa1 # 1
b -> pb1 # 2

ball[pa1, pa1] -> qa1
ball[pb1, pb1] -> qb1

final qa1, qb1